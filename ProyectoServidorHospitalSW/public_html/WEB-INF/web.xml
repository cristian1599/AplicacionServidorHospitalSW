<?xml version = '1.0' encoding = 'windows-1252'?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
  <servlet>
    <servlet-name>PacienteSWPort</servlet-name>
    <servlet-class>serviciosWeb.PacienteSW</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>HistorialClinicoSWPort</servlet-name>
    <servlet-class>serviciosWeb.HistorialClinicoSW</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>ServicioEnfermedadSWPort</servlet-name>
    <servlet-class>serviciosWeb.EnfermedadSW</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>PacienteSWPort</servlet-name>
    <url-pattern>/PacienteSWPort</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>HistorialClinicoSWPort</servlet-name>
    <url-pattern>/HistorialClinicoSWPort</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServicioEnfermedadSWPort</servlet-name>
    <url-pattern>/ServicioEnfermedadSWPort</url-pattern>
  </servlet-mapping>
</web-app>
